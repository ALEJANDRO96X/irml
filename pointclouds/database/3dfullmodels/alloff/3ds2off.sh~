#!/bin/sh
# By CaDtUxJaK
# Renombrar los espacios de los archivos a _
find . -name ‘* *’ | sort | while read FILE
do
NEWFILE=`echo ${FILE} | sed ’s/ /_/g;’`
mv “${FILE}” ${NEWFILE}
echo ${NEWFILE}
done
# Convertir png a pdf, es necesario el convert de Imagemagik
for file in *.png
do
convert $file `basename $file .png`.pdf;
done
